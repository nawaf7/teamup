<!DOCTYPE html>
<html>

  <head>

    <title>Home Page</title>

  </head>

  <body>

      <h1>Welcome to the Home Page</h1>

          <div>
            <% if !current_user %>
              <%= link_to 'User', new_user_session_path, :class => ""%>
            <% end %>
          </div>

          <div>
            <% if current_user %>
              <%= link_to 'My Profile', user_path(current_user) %>
              <%= link_to 'logout', destroy_user_session_path, method: :delete %>
            <% end %>
          </div>

  </body>

</html>